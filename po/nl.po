#: app/main.c:416
#, c-format
msgid ""
"\n"
"Invalid option \"%s\"\n"
msgstr ""
"\n"
"Ongeldige optie \"%s\"\n"

#: app/main.c:504
msgid "GIMP version"
msgstr "GIMP-versie"

#: app/main.c:512
#, c-format
msgid ""
"\n"
"Usage: %s [option ... ] [file ... ]\n"
"\n"
