#, c-format
msgid "Could not open '%s' for writing: %s"
msgstr "Ei saanud '%s' kirjutamiseks avada: %s"

msgid "Error parsing '%s': line longer than %"
msgstr "Viga '%s' analüüsimisel: rida on pikem kui %"

#, c-format
