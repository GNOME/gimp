#: ../app/about.h:23
msgid "GIMP"
msgstr "GIMP"

#: ../app/about.h:26 ../desktop/gimp.desktop.in.in.h:1
msgid "GNU Image Manipulation Program"
msgstr "Πρόγραμμα επεξεργασίας εικόνων GNU"

