msgctxt "align-reference-type"
msgid "First item"
msgstr "Primo elemento"

#: ../app/core/core-enums.c:28
msgctxt "align-reference-type"
msgid "Image"
msgstr "Immagine"

#: ../app/core/core-enums.c:29
msgctxt "align-reference-type"
msgid "Selection"
msgstr "Selezione"

#: ../app/core/core-enums.c:30
msgctxt "align-reference-type"
