-static CTX_INLINE float ctx_atan2f (const float y, const float x)
-{
-  float atan, z;
-  if ( x == 0.0f )
-    {
-      if ( y > 0.0f )
-        { return CTX_PI/2; }
-      if ( y == 0.0f )
-        { return 0.0f; }
-      return -CTX_PI/2;
-    }
-  z = y/x;
-  if ( ctx_fabsf ( z ) < 1.0f )
-    {
-      atan = z/ (1.0f + 0.28f*z*z);
-      if (x < 0.0f)
-        {
-          if ( y < 0.0f )
-            { return atan - CTX_PI; }
-          return atan + CTX_PI;
-        }
-    }
-  else
-    {
-      atan = CTX_PI/2 - z/ (z*z + 0.28f);
-      if ( y < 0.0f ) { return atan - CTX_PI; }
-    }
-  return atan;
-}
