-       for (int i = 0; i < old_size; i++)
-         if (old[i])
-           sqz_pool_add_entry (pool, old[i]);
-       free (old);
-     }
-  }
-  pool->count++;
-
-  int pos = (str->hash/2) & (pool->size-1);
-  while (pool->hashtable[pos])
-  {
-    pos++;
-    pos &= (pool->size-1);
-  }
-  pool->hashtable[pos]=str;
-  return pos;
