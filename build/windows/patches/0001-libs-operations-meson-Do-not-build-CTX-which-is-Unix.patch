-  if (tcsetattr (fd, TCSAFLUSH, &raw) < 0)
-    return -1;
-  if (fd == STDIN_FILENO)
-    nc_is_raw = 1;
-#if !__COSMOPOLITAN__
-  tcdrain(fd);
