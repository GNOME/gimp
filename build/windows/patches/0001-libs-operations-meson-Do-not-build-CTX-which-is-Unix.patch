-static inline uint32_t MurmurOAAT32 (const char * key, int len)
-{
-  size_t h = 3323198485ul;
-  for (int i = 0;i < len;i++) {
-    h ^= key[i];
-    h *= 0x5bd1e995;
-    h &= 0xffffffff;
-    h ^= h >> 15;
-  }
-  return h;
-}
-
-static inline uint64_t MurmurOAAT64 ( const char * key, int len)
-{
-  uint64_t h = 525201411107845655ull;
-  for (int i = 0;i < len;i++) {
-    h ^= key[i];
-    h *= 0x5bd1e9955bd1e995;
-    h ^= h >> 47;
-  }
-  return h;
-}
