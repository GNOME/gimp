-void
-ctx_string_append_int (CtxString *string, int val)
-{
-  char buf[64];
-  char *bp = &buf[0];
-  int remainder;
-  if (val < 0)
-  {
-    buf[0]='-';
-    bp++;
-    remainder = -val;
-  }
-  else
-  remainder = val;
-
-  int len = 0;
-  do {
-    int digit = remainder % 10;
-    bp[len++] = digit + '0';
-    remainder /= 10;
-  } while (remainder);
-
-  bp[len]=0;
-  for (int i = 0; i < len/2; i++)
-  {
-    int tmp = bp[i];
-    bp[i] = bp[len-1-i];
-    bp[len-1-i] = tmp;
-  }
-  len += (val < 0);
-  ctx_string_append_str (string, buf);
-}
