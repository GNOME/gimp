-static void ctx_float_set_sat (int components, float *c, float sat)
-{
-  int max = 0, mid = 1, min = 2;
-  
-  if (c[min] > c[mid]){int t = min; min = mid; mid = t;}
-  if (c[mid] > c[max]){int t = mid; mid = max; max = t;}
-  if (c[min] > c[mid]){int t = min; min = mid; mid = t;}
-
-  if (c[max] > c[min])
-  {
-    c[mid] = ((c[mid]-c[min]) * sat) / (c[max] - c[min]);
-    c[max] = sat;
-  }
-  else
-  {
-    c[mid] = c[max] = 0.0f;
-  }
-  c[min] = 0.0f;
-
-}
