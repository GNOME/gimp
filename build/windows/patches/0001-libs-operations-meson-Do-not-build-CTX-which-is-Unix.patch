-static int ctx_css_compute_specifity (const char *selector, int priority)
-{
-  const char *p;
-  int n_id = 0;
-  int n_class = 0;
-  int n_tag = 0;
-  int n_pseudo = 0;
-  int in_word = 0;
-
-  for (p = selector; *p; p++)
-  {
-    switch (*p)
-    {
-      case ' ':
-        in_word = 0;
-        break;
-      case ':':
-        in_word = 1;
-        n_pseudo++;
-        break;
-      case '.':
-        in_word = 1;
-        n_class++;
-        break;
-      case '#':
-        in_word = 1;
-        n_id++;
-        break;
-      default:
-        if (!in_word)
-        {
-          in_word = 1;
-          n_tag++;
-        }
-    }
-  }
-  return priority * 100000 + n_pseudo * 10000 + n_id * 1000 + n_class * 100 + n_tag * 10;
-}
