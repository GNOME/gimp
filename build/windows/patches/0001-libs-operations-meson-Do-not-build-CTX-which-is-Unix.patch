-      if (o[j] == 0 ||
-          o[j] == ';')
-        got_more = 0;
-      else
-        got_more = 1;
-      o[j]=0; /* XXX: this is where foo;bar won't work but foo ;bar works*/
-
-      if(eat)
-       {
-         int k;
