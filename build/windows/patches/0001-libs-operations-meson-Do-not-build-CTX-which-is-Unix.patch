-  {
-    case 3:
-    case 4:
-            { float r = c[0];
-              float g = c[1];
-              float b = c[2];
-              return ctx_maxf(r, ctx_maxf(g,b)) - ctx_minf(r,ctx_minf(g,b));
-            }
-            break;
-    case 1:
-    case 2: return 0.0;
-            break;
-    default:
-       {
-         float min = 1000;
-         float max = -1000;
-         for (int i = 0; i < components - 1; i ++)
-         {
-           if (c[i] < min) min = c[i];
-           if (c[i] > max) max = c[i];
