-static void ctx_edge_qsortb (CtxSegment *entries, int low, int high)
-{
-  do {
-    int p = ctx_edge_qsort_partition (entries, low, high);
-    if (low < p - 1)
-      ctx_edge_qsortb (entries, low, p - 1);
-    if (low >= high)
-      return;
-    low = p;
-  } while (1);
