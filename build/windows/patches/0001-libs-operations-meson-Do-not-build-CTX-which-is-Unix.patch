-    {
-      small = parser->config.width;
-      big   = parser->config.height;
-    }
-  switch (code)
-    {
-      case CTX_RADIAL_GRADIENT:
-      case CTX_ARC:
-        switch (arg_no)
-          {
-            case 0:
-            case 3:
-              *value *= (parser->config.width/100.0f);
-              break;
-            case 1:
-            case 4:
-              *value *= (parser->config.height/100.0f);
-              break;
-            case 2:
-            case 5:
-              *value *= small/100.0f;
-              break;
