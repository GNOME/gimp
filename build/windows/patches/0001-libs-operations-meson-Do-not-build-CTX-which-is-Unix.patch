-            case ':':
-              {
-                int i = 0;
-                for (i = 0; i < CTX_STYLE_MAX_PSEUDO && node->pseudo[i]; i++);
-                node->pseudo[i] = mrg_intern_string (&temp[1]);
-                for (i = 0; i < CTX_STYLE_MAX_PSEUDO && node->pseudo_hash[i]; i++);
