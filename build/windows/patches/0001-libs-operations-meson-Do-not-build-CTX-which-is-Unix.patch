-  SDL_Event event;
-
-  while (SDL_PollEvent (&event))
-  {
-    switch (event.type)
-    {
-      case SDL_MOUSEBUTTONDOWN:
-        SDL_CaptureMouse (SDL_TRUE);
-        ctx_pointer_press (ctx, event.button.x, event.button.y, event.button.button, 0);
-        break;
-      case SDL_MOUSEBUTTONUP:
-        SDL_CaptureMouse (SDL_FALSE);
