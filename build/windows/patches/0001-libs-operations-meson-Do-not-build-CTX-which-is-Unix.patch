-static inline int _ctx_nth_match (const char *selector, int child_no)
-{
-  const char *tmp = selector + 10;
-  int a = 0;
-  int b = 0;
-
-  if (!strcmp (tmp, "odd)"))
-  {
-    a = 2; b = 1;
-  }
-  else if (!strcmp (tmp, "even)"))
-  {
-    a = 2; b = 0;
-  }
-  else
-  {
-    if (ctx_strchr (tmp, 'n'))
-    {
-      a = atoi (tmp);
-      b = atoi (ctx_strchr (tmp, 'n')+1);
-    }
-    else
-    {
-      b = atoi (tmp);
-    }
-  }
-
-  if (!a)
-    return child_no == b;
-  else
-    if (child_no == b ||
-       ((child_no - b > 0) == (a>0)))
-      return !((child_no - b) % a);
-
-  return 0;
-}
