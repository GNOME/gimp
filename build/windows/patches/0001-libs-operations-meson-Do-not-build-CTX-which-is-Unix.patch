-
-  gegl_path_get_bounds (o->d, &x0, &x1, &y0, &y1);
-  defined.x      = x0;
-  defined.y      = y0;
-  defined.width  = x1 - x0;
-  defined.height = y1 - y0;
-
-  if (in_rect)
-    {
-      gegl_rectangle_bounding_box (&defined, &defined, in_rect);
-    }
-
-  return defined;
