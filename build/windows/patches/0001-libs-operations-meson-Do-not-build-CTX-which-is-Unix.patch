-    }
-    else
-#endif
-    {
-      CtxEntry *commands;
-      int command_size = 1 + (data_len+1+1)/9 + 1 + (eid_len+1+1)/9 + 1 +   8;
-      if (ctx->backend && (void*)ctx->backend->process != (void*)ctx_drawlist_process)
-      {
-         commands = (CtxEntry*)ctx_calloc (command_size, sizeof (CtxEntry));
-      }
