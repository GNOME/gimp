-static inline float ctx_p_line_sq_dist (float x, float y, float x1, float y1, float x2, float y2) {
-  float A = x - x1;
-  float B = y - y1;
-  float C = x2 - x1;
-  float D = y2 - y1;
-
-  float dot = A * C + B * D;
-  float len_sq = C * C + D * D;
-  float param = -1.0f;
-  float xx, yy;
-
-  if (len_sq != 0.0f) //in case of 0 length line
-      param = dot / len_sq;
-
-  if (param < 0.0f) {
-    xx = x1;
-    yy = y1;
-  }
-  else if (param > 1.0f) {
-    xx = x2;
-    yy = y2;
-  }
-  else {
-    xx = x1 + param * C;
-    yy = y1 + param * D;
-  }
-
-  float dx = x - xx;
-  float dy = y - yy;
-  return dx * dx + dy * dy;
-}
