-static void
-ctx_normalize (float *x, float *y)
-{
-  float length = ctx_hypotf ( (*x), (*y) );
-  if (length > 1e-6f)
-    {
-      float r = 1.0f / length;
-      *x *= r;
-      *y *= r;
-    }
-}
