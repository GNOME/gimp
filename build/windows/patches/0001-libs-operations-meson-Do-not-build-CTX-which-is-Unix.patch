-  GeglProperties *o = GEGL_PROPERTIES (operation);
-  Ctx     *ctx = ctx_new_drawlist (-1, -1);
-  gboolean result = FALSE;
-
-  gegl_path_ctx_play (o->d, ctx);
-
-  if (!result)
-    {
-      if (o->d)
-        {
-          result = ctx_in_fill (ctx, x, y);
-        }
-    }
-  ctx_free (ctx);
-
-  if (result)
-    return operation->node;
-
-  return NULL;
-}
