-    return;
-
-  for (int i = 0; i < 255; i ++)
-    base64_revmap[i]=255;
-  for (int i = 0; i < 64; i ++)
-    base64_revmap[((const unsigned char*)base64_map)[i]]=i;
-  /* include variants used in URI encodings for decoder,
-   * even if that is not how we encode
-  */
-  base64_revmap['-']=62;
