-  float y   = mrg_y (mrg);
-  float em  = mrg_em (mrg);
-  int i;
-
-  if (mrg->floats)
-    for (i = state->float_base; i < mrg->floats; i++)
-    {
-      CtxFloatData *f = &mrg->float_data[i];
-      if (f->type == CTX_FLOAT_RIGHT &&
-          y >= f->y  &&
-          y - em < f->y + f->height &&
-
-          f->x < ret)
-          ret = f->x;
-    }
-  return ret;
-}
