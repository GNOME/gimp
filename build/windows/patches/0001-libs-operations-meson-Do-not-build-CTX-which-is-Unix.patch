-    ctx_sha1_process (sha1, (uint8_t*)eid, eid_len);
-    ctx_sha1_done (sha1, hash);
-    ctx_sha1_free (sha1);
-    const char *hex="0123456789abcdef";
-    for (int i = 0; i < 20; i ++)
-    {
-       ascii[i*2]  = hex[hash[i]/16];
-       ascii[i*2+1]= hex[hash[i]%16];
-    }
-    ascii[40] = 0;
-    eid = ascii;
-    eid_len = 40;
-  }
-
-  if (ret_eid && (ret_eid != eid))
-  {
-    strcpy (ret_eid, eid);
