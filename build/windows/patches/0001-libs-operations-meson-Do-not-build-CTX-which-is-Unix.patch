-static int ctx_isxdigit (uint8_t ch)
-{
-  if (ch >= '0' && ch <= '9') return 1;
-  if (ch >= 'a' && ch <= 'f') return 1;
-  if (ch >= 'A' && ch <= 'F') return 1;
-  return 0;
-}
