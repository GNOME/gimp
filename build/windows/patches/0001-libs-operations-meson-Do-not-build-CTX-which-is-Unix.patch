-static inline float _ctx_parse_float (const char *str, char **endptr)
-{
-  float result = 0;      
-  if (!str){
-    if (endptr)
-      *endptr = NULL;
-    return 0.0;
-  }
-  while (*str == ' ') str++;
-  result = atoi (str);
-
-  const char *period = NULL;
-
-  {
-    const char *p = str;
-    for (; *p && *p >= '0' && *p <= '9';p++);
-    if (*p == '.') period = p;
-  }
-
-  if (period)
-  {     
-    const char *p = period + 1;
-    float d = 10;  
-    for (;*p && *p >= '0' && *p <= '9';p++, d *= 10)  
-    {   
-      if (result >= 0)  
-        result += (*p - '0') / d;     
-      else                           
-        result -= (*p - '0') / d;  
-    } 
-    if (endptr)
-      *endptr = (char*)p;
-  }                  
-  else
-  {
-    const char *p = str;
-    for (;*p && *p >= '0' && *p <= '9';p++);
-    if (endptr)
-      *endptr = (char*)p;
-  }
-  return result;
-}
