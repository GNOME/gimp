-static inline int mrg_parse_pxs (Css *mrg, const char *str, float *vals)
-{
-  int n_floats = 0;
-  char *p =    (char*)str;
-  char *prev = (char*)NULL;
-
-  for (; p && p != prev && *p; )
-  {
-    float val;
-    prev = p;
-    val = n_floats%2==1?
-      mrg_parse_px_x (mrg, p, &p):mrg_parse_px_y (mrg, p, &p);
-    if (p != prev)
-    {
-      vals[n_floats++] = val;
-    }
-  }
-
-  return n_floats;
-}
