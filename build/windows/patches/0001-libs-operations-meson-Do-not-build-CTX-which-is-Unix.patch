-static inline int mrg_utf8_len (const unsigned char first_byte)
-{
-  if      ((first_byte & 0x80) == 0)
-    return 1; /* ASCII */
-  else if ((first_byte & 0xE0) == 0xC0)
-    return 2;
-  else if ((first_byte & 0xF0) == 0xE0)
-    return 3;
-  else if ((first_byte & 0xF8) == 0xF0)
-    return 4;
-  return 1;
-}
