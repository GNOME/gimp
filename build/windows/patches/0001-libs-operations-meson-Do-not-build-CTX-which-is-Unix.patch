-  }
-
-  /* then we look for a match of the substring after the first
-   * space
-   */
-  if (ret < 0 && strchr (name, ' '))
-  {
-     char *subname = (char*)strchr (name, ' ');
-     for (int i = 0; ret < 0 && i < ctx_font_count; i ++)
