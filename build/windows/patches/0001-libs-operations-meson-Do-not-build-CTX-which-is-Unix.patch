- * marked up xml for the highlighting - it limits the syntax highlighting
- * context ability
- */
-enum {
-  MRG_HL_NEUTRAL      = 0,
-  MRG_HL_NEXT_NEUTRAL = 1,
-  MRG_HL_STRING       = 2,
-  MRG_HL_STRING_ESC   = 3,
-  MRG_HL_QSTRING      = 4,
-  MRG_HL_QSTRING_ESC  = 5,
-  MRG_HL_SLASH        = 6,
-  MRG_HL_LINECOMMENT  = 7,
-  MRG_HL_COMMENT      = 8,
-  MRG_HL_COMMENT_STAR = 9,
-};
