-  if (valid && (eid[0] == '!') && ctx->texture_cache)
-  {
-    for (int i = 0; i < CTX_MAX_TEXTURES; i++)
-    {
-      CtxBuffer *buffer = &(ctx->texture_cache->texture[i]);
