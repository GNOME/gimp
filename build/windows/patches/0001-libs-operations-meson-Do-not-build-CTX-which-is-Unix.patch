-  char temp[128] = "";
-  unsigned int  temp_l = 0;
-  if (!style_id)
-  {
-    return; // XXX: why does this happen?
-  }
-
-  memset (node, 0, sizeof (CtxStyleNode));
-
-  for (p = style_id; ; p++)
-  {
-    switch (*p)
-    {
-      case '.':
-      case ':':
-      case '#':
-      case 0:
-        if (temp_l)
-        {
-          switch (temp[0])
-          {
-            case '.':
-              {
-                int i = 0;
-                for (i = 0; i < CTX_STYLE_MAX_CLASSES && node->classes_hash[i]; i++);
-                node->classes_hash[i] = ctx_strhash (&temp[1]);
-              }
