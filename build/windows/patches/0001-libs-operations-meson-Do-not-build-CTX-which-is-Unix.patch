-  
-      pos = 2 + 1 + ctx_conts_for_entry (&commands[2]);
-      commands[pos].code        = CTX_DATA;
-      commands[pos].data.u32[0] = data_len;
-      commands[pos].data.u32[1] = (data_len+1+1)/9 + 1;
