-    }
-  }
-  if (!last)
-    return;
-  if (prevlast == client)
-    return;
-  ctx_list_remove (&ctx->events.clients, client);
-  ctx_list_append (&ctx->events.clients, client);
-  ctx_list_remove (&ctx->events.clients, last);
-  ctx_list_append (&ctx->events.clients, last);
