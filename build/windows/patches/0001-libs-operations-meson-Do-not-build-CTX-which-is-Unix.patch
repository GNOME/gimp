-        SqzPool *pool = &global_pool;
-        if (sqz_pool_remove (pool, squozed, 1))
-        {
-          return;
-        }
-        pool = sqz_pools;
-        if (pool)
-        do {
-          if (sqz_pool_remove (pool, squozed, 1))
-          {
-            return;
-          }
-          pool = pool->next;
-        } while (pool);
