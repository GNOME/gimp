-    for (int y = 0; y < height; y++)
-    {
-       ctx_sha1_process (sha1, src, dst_stride);
-       src += stride;
-    }
-    ctx_sha1_done (sha1, hash);
-    ctx_sha1_free (sha1);
-    const char *hex="0123456789abcdef";
-    for (int i = 0; i < 20; i ++)
-    {
-       ascii[i*2]   = hex[hash[i]/16];
-       ascii[i*2+1] = hex[hash[i]%16];
