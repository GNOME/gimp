-void mrg_hl_text (Ctx *cr, const char *text)
-{
-  int i;
-  CtxString *word = ctx_string_new ("");
-  for (i = 0; i < text[i]; i++)
-  {
-    switch (text[i])
-    {
-      case ';':
-      case '-':
-      case '\'':
-      case '>':
-      case '<':
-      case '=':
-      case '+':
-      case ' ':
-      case ':':
-      case '"':
-      case '*':
-      case '/':
-      case '\\':
-      case '[':
-      case ']':
-      case ')':
-      case ',':
-      case '(':
-        if (word->length)
-        {
-          mrg_hl_token (cr, word->str);
-          ctx_string_set (word, "");
