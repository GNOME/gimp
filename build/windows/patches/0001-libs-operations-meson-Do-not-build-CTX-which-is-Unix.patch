-static void
-ctx_fontgen_real_add_glyphs (CtxFontGenerator *gen)
-{
-  qsort (gen->incoming_glyphs, gen->n_incoming_glyphs, sizeof (uint32_t),
-         ctx_fontgen_compare_glyphs);
-  for (unsigned int i = 0; i < gen->n_incoming_glyphs; i++)
