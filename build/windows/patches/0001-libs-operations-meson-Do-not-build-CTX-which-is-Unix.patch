-      case CTX_STROKE_POS:
-      case CTX_FEATHER:
-      case CTX_FONT_SIZE:
-      case CTX_MITER_LIMIT:
-      case CTX_LINE_WIDTH:
-      case CTX_LINE_DASH_OFFSET:
-        {
-          *value *= (small/100.0f);
