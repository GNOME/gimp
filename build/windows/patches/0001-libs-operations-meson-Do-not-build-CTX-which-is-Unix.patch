-                return NULL;
-              }
-              return match->nick;
-              break;
-            case 9001: /* mouse event */
-              if (x) *x = ((unsigned char)buf[4]-32);
-              if (y) *y = ((unsigned char)buf[5]-32);
-              switch (buf[3])
-                {
-                        /* XXX : todo reduce this to less string constants */
-                  case 32:  return "pp";
-                  case 33:  return "mouse1-press";
-                  case 34:  return "mouse2-press";
