-ctx_base642bin (const char    *ascii,
-                int           *length,
-                unsigned char *bin)
-{
-  int i;
-  int charno = 0;
-  int outputno = 0;
-  int carry = 0;
-  base64_revmap_init ();
-  for (i = 0; ascii[i]; i++)
-    {
-      int bits = base64_revmap[((const unsigned char*)ascii)[i]];
-      if (length && outputno > *length)
-        {
-          *length = -1;
-          return -1;
-        }
-      if (bits != 255)
-        {
-          switch (charno % 4)
-            {
-              case 0:
-                carry = bits;
-                break;
-              case 1:
-                bin[outputno] = (carry << 2) | (bits >> 4);
-                outputno++;
-                carry = bits & 15;
-                break;
-              case 2:
-                bin[outputno] = (carry << 4) | (bits >> 2);
-                outputno++;
