-  sprintf (buf, "\033[_Af=%i;", frames);
-  vt_write (vt, buf, strlen (buf));
-  vt_write (vt, encoded, strlen(encoded));
-  ctx_free (encoded);
-
-  if (data != samples)
-    ctx_free (data);
-
-  //vt_write (vt, samples, bytes);
-  buf[0]='\033';
-  buf[1]='\\';
-  buf[2]=0;
