-static int ydec (const void *srcp, void *dstp, int count)
-{
-  const char *src = srcp;
-  char *dst = dstp;
-  int out_len = 0;
-  for (int i = 0; i < count; i ++)
-  {
-    int o = src[i];
-    switch (o)
-    {
-      case '=':
-              i++;
-              o = src[i];
-              o = (o-42-64) % 256;
-              break;
-      case '\n':
-      case '\033':
-      case '\r':
-      case '\0':
-              break;
-      default:
-              o = (o-42) % 256;
-              break;
-    }
-    dst[out_len++] = o;
-  }
-  dst[out_len]=0;
-  return out_len;
-}
