-              int encoded_val;
-              if (needed_jump < -2)
-                encoded_val = 5 - needed_jump;
-              else
-                encoded_val = needed_jump - 3;
-
-              ADD_QUINTET ((encoded_val/4) + SQUOZE_DEC_OFFSET_A);
