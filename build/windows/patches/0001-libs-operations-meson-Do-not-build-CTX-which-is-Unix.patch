-         prev_drag_y = ctx_mt[i].y;
-       }
-
-
-       ctx_mt[i].reported_id = ctx_mt[i].id;
-       ctx_mt[i].reported_x  = ctx_mt[i].x;
-       ctx_mt[i].reported_y  = ctx_mt[i].y;
-      }
-    }
