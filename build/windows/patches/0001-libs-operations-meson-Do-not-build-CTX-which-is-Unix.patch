-static void bin2base64_group (const unsigned char *in, int remaining, char *out)
-{
-  unsigned char digit[4] = {0,0,64,64};
-  int i;
-  digit[0] = in[0] >> 2;
-  digit[1] = ((in[0] & 0x03) << 4) | ((in[1] & 0xf0) >> 4);
-  if (remaining > 1)
-    {
-      digit[2] = ((in[1] & 0x0f) << 2) | ((in[2] & 0xc0) >> 6);
-      if (remaining > 2)
-        digit[3] = ((in[2] & 0x3f));
-    }
-  for (i = 0; i < 4; i++)
-    out[i] = base64_map[digit[i]];
-}
