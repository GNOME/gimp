-  if (!client) return;
-  for (CtxList *l = ctx->events.clients; l; l=l->next)
-  {
-    prevlast = last;
-    last = (CtxClient*) l->data;
-  }
-  if (client == last)
-  {
-    if (prevlast)
-    {
-      ctx_list_remove (&ctx->events.clients, prevlast);
