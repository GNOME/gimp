      gimp_drawable_apply_operation_by_name (drawable, progress,
                                             C_("undo-type", "Color Balance"),
                                             "gimp:color-balance",
                                             config);
      g_object_unref (config);
    }
  else
    success = FALSE;
}
CODE
    );
}

