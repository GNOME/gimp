    %invoke = (
        code => <<'CODE'
{
  GimpVectorLayerOptions *options;

  options = gimp_vector_layer_get_options (layer);
  if (! options)
    success = FALSE;

  if (success)
    gimp_context_set_foreground (GIMP_CONTEXT (options->fill_options),
                                 color);
}
CODE
    );
}

