"PDB_IMAGE"
"Input image (unused)"
 ) ( "drawable"
"PDB_DRAWABLE"
"Input drawable"
 ) ( "period"
"PDB_INT32"
"period; number of pixels for one wave to complete"
 ) ( "amplitude"
"PDB_INT32"
"amplitude; maximum displacement of wave"
 ) ( "orientation"
"PDB_INT32"
"orientation; 0 = Horizontal, 1 = Vertical"
 ) ( "edges"
"PDB_INT32"
"edges; 0 = smear, 1 =  wrap, 2 = black"
 ) ( "waveform"
"PDB_INT32"
"0 = sawtooth, 1 = sine wave"
 ) ( "antialias"
"PDB_INT32"
"antialias; True or False"
